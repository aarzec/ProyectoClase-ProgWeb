@{
    ViewData["Title"] = "Index";
}

<h2>Guardar Tipo Medicamento</h2>
<form class="row g-3" id="formGuardarTipoMedicamento">
    <div class="col-md-6">
        <label for="idTipoMedicamento" class="form-label">Id Tipo Medicamento</label>
        <input type="text" name="idTipoMedicamento" class="form-control" id="idTipoMedicamento" readonly value="">
    </div>
    <div class="col-md-6">
        <label for="nombre" class="form-label">Nombre</label>
        <input type="text" name="nombre" class="form-control" id="nombre" value="">
    </div>
    <div class="col-md-6">
        <label for="descripcion" class="form-label">Descripción</label>
        <input type="text" name="descripcion" class="form-control" id="descripcion" value="">
    </div>
    <div class="col-md-3">
        <button class="btn btn-outline-secondary" type="button" onclick="guardarTipoMedicamento()" id="buttonGuardar">Guardar</button>
        <button class="btn btn-outline-info" type="button" onclick="limpiarTipoMedicamento()">Limpiar</button>
    </div>
</form>
<div id="divtablaBusqueda">
</div>

<hr />

