<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - PrimeraAppNetCoreMVC</title>
    <link rel="stylesheet" href="~/PrimeraAppNetCoreMVC.styles.css" asp-append-version="true" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.7.1/jquery.min.js" ></script>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/2.1.4/toastr.min.js"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/MiPrimeraAppMVC.styles.css" asp-append-version="true" />
    <link href="//cdn.datatables.net/2.2.2/css/dataTables.dataTables.min.css" rel="stylesheet" />
    <script src="//cdn.datatables.net/2.2.2/js/dataTables.min.js"></script>
    <link href="~/css/nav.css" rel="stylesheet" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css" rel="stylesheet" />
</head>
<body>
    <header>
        <nav class="navbar">
            <ul class="navbar__menu">
                <li class="navbar__item">
                    <a asp-area="" asp-controller="Home" asp-action="Index" class="navbar__link"><i data-feather="home"></i><span>Inicio</span></a>
                </li>
                <li class="navbar__item">
                    <a asp-area="" asp-controller="TipoMedicamento" asp-action="sinMenu" class="navbar__link"><i data-feather="message-square"></i><span>Sin menú</span></a>
                </li>
                <li class="navbar__item">
                    <a asp-area="" asp-controller="Home" asp-action="Privacy" class="navbar__link"><i data-feather="users"></i><span>Privacidad</span></a>
                </li>
                <li class="navbar__item">
                    <a asp-area="" asp-controller="TipoMedicamento" asp-action="Index" class="navbar__link"><i data-feather="folder"></i><span>Tipo Medicamento</span></a>
                </li>
                <li class="navbar__item">
                    <a asp-area="" asp-controller="Sucursal" asp-action="Index" class="navbar__link"><i data-feather="archive"></i><span>Sucursal</span></a>
                </li>
                <li class="navbar__item">
                    <a asp-area="" asp-controller="Laboratorio" asp-action="Index" class="navbar__link"><i data-feather="help-circle"></i><span>Laboratorio</span></a>
                </li>
                <li class="navbar__item">
                    <a asp-area="" asp-controller="Documento" asp-action="Index" class="navbar__link"><i data-feather="settings"></i><span>Documento</span></a>
                </li>
            </ul>
        </nav>
    </header>
    <input type="hidden" id="hdfOculto" value="@Url.Content("~")" />
    <div class="container">
        <main role="main" class="p-3">
            @RenderBody()
        </main>
    </div>
    <script src="https://unpkg.com/feather-icons@4.29.2/dist/feather.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
